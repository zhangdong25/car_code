tive=\"currentStatusIndex\" :space=\"100\" direction=\"vertical\">\r\n          <el-step v-for=\"(status, index) in logisticsStatus\" :key=\"index\" :title=\"status.time\" :description=\"status.message\"></el-step>\r\n        </el-steps>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        orderNumber: ''\r\n      },\r\n      logisticsStatus: null,\r\n      orderNumbers: [\r\n        'ORD123456',\r\n        '202307150001',\r\n        'ABC7890123',\r\n        '202307150002',\r\n        'XYZ4567890'\r\n        // 添加其他虚拟的订单编号\r\n      ],\r\n      currentStatusIndex: 0\r\n    };\r\n  },\r\n  methods: {\r\n    searchLogistics() {\r\n      // 在这里执行向后端或物流服务商请求查询物流信息的操作\r\n      // 假设在这里模拟获取物流状态更新的数据\r\n      const orderNumber = this.form.orderNumber;\r\n      if (this.orderNumbers.includes(orderNumber)) {\r\n        this.logisticsStatus = [\r\n          { time: '2023-07-15 10:00', message: '您的订单已发货' },\r\n          { time: '2023-07-15 12:00', message: '货物已到达目的地城市' },\r\n          { time: '2023-07-15 14:00', message: '正在派送中' },\r\n          { time: '2023-07-15 16:00', message: '订单已签收' },\r\n          // 添加更多物流状态...\r\n        ];\r\n        this.currentStatusIndex = 0;\r\n      } else {\r\n        this.logisticsStatus = null;\r\n        this.currentStatusIndex = 0;\r\n        alert('未找到该订单编号，请输入正确的订单编号！');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.logistics-container {\r\n  max-height: 300px;\r\n  /* 设置容器的最大高度，超出部分将显示滚动条 */\r\n  overflow-y: auto;\r\n  /* 显示垂直滚动条 */\r\n}\r\n\r\n.card-header {\r\n  background-color: #f5f7fa;\r\n  padding: 15px;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  border-bottom: 1px solid #ebeef5;\r\n}\r\n</style>\r\n"],"mappings":"AA0BA;EACAA,KAAA;IACA;MACAC,IAAA;QACAC,WAAA;MACA;MACAC,eAAA;MACAC,YAAA,GACA,aACA,gBACA,cACA,gBACA;MACA;MAAA,CACA;;MACAC,kBAAA;IACA;EACA;EACAC,OAAA;IACAC,gBAAA;MACA;MACA;MACA,MAAAL,WAAA,QAAAD,IAAA