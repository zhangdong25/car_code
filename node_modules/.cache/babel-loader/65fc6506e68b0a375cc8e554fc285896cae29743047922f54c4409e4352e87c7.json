{"ast":null,"code":"export default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      passwordCopy: '',\n      phone: '',\n      select: '登录',\n      login: true,\n      value: '',\n      options: [{\n        value: '选项1',\n        label: '买家'\n      }, {\n        value: '选项',\n        label: ''\n      }]\n    };\n  },\n  methods: {\n    login() {\n      alert(`用户名为：${this.username},用户密码为：${this.password},${this.select}`);\n    },\n    register() {\n      if (this.username == '' || this.password == '' || this.passwordCopy == '' || this.phone == '') {\n        this.$message.error('填写所有信息，才可以提交!');\n      } else {\n        if (this.password != this.passwordCopy) {\n          this.$message.error('两次密码不一致!');\n        } else {\n          this.$message({\n            message: '注册成功!',\n            type: 'success'\n          });\n        }\n      }\n    }\n  },\n  watch: {\n    'select': {\n      handler(nval, oval) {\n        nval === \"注册\" ? this.login = false : this.login = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","username","password","passwordCopy","phone","select","login","value","options","label","methods","alert","register","$message","error","message","type","watch","handler","nval","oval"],"sources":["src/pages/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div>\r\n      <el-radio-group size=\"medium\" v-model=\"select\">\r\n        <el-radio-button label=\"登录\"></el-radio-button>\r\n        <el-radio-button label=\"注册\"></el-radio-button>\r\n      </el-radio-group>\r\n    </div>\r\n    <div id=\"login\" v-if=\"login\">\r\n      <el-input placeholder=\"请输入账号\" clearable v-model=\"username\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <el-input placeholder=\"请输入密码\" show-password v-model=\"password\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <el-button type=\"success\" round @click=\"login()\">登录</el-button>\r\n    </div>\r\n    <div id=\"register\" v-if=\"!login\">\r\n      <el-input placeholder=\"请输入账号\" clearable v-model=\"username\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <el-input placeholder=\"请输入密码\" show-password v-model=\"password\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <el-input placeholder=\"确认密码\" show-password v-model=\"passwordCopy\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <div id=\"role\">\r\n        <el-select v-model=\"value\" placeholder=\"请选择身份\">\r\n          <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n          </el-option>\r\n        </el-select>\r\n      </div>\r\n      <el-input placeholder=\"请输入联系方式\" v-model=\"phone\" style=\"width: 70%; height: 45px;\"></el-input>\r\n      <el-button type=\"primary\" round @click=\"register()\">提交</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n      passwordCopy: '',\r\n      phone: '',\r\n      select: '登录',\r\n      login: true,\r\n      value: '',\r\n      options: [{\r\n        value: '选项1',\r\n        label: '买家',\r\n      }, {\r\n        value: '选项',\r\n        label: ''\r\n      }]\r\n    }\r\n  },\r\n  methods: {\r\n    login() {\r\n      alert(`用户名为：${this.username},用户密码为：${this.password},${this.select}`)\r\n    },\r\n    register() {\r\n      if (this.username == '' || this.password == '' || this.passwordCopy == '' || this.phone == '') {\r\n        this.$message.error('填写所有信息，才可以提交!');\r\n      } else {\r\n        if (this.password != this.passwordCopy) {\r\n          this.$message.error('两次密码不一致!');\r\n        } else {\r\n          this.$message({\r\n            message: '注册成功!',\r\n            type: 'success'\r\n          });\r\n        }\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    'select': {\r\n      handler(nval, oval) {\r\n        nval === \"注册\" ? this.login = false : this.login = true;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.content {\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background-color: #f5f5f5;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n#login {\r\n  width: 35%;\r\n  height: 50%;\r\n  border-radius: 3%;\r\n  background-color: #F2F6FC;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n}\r\n\r\n#register {\r\n  width: 35%;\r\n  height: 60%;\r\n  border-radius: 3%;\r\n  background-color: #F2F6FC;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n}\r\n\r\n#role {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 70%;\r\n  height: 45px;\r\n  background-color: aquamarine;\r\n  border-radius: 18px;\r\n}\r\n\r\n/deep/ .el-input__inner {\r\n  border-radius: 18px;\r\n  height: 100%;\r\n}\r\n</style>"],"mappings":"AA8BA;EACAA,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,YAAA;MACAC,KAAA;MACAC,MAAA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;QACAD,KAAA;QACAE,KAAA;MACA;QACAF,KAAA;QACAE,KAAA;MACA;IACA;EACA;EACAC,OAAA;IACAJ,MAAA;MACAK,KAAA,cAAAV,QAAA,eAAAC,QAAA,SAAAG,MAAA;IACA;IACAO,SAAA;MACA,SAAAX,QAAA,eAAAC,QAAA,eAAAC,YAAA,eAAAC,KAAA;QACA,KAAAS,QAAA,CAAAC,KAAA;MACA;QACA,SAAAZ,QAAA,SAAAC,YAAA;UACA,KAAAU,QAAA,CAAAC,KAAA;QACA;UACA,KAAAD,QAAA;YACAE,OAAA;YACAC,IAAA;UACA;QACA;MACA;IACA;EACA;EACAC,KAAA;IACA;MACAC,QAAAC,IAAA,EAAAC,IAAA;QACAD,IAAA,iBAAAb,KAAA,gBAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}